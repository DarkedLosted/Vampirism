                <Label id="Title" text="#BuildMenu"/>
            </Panel>
            <Panel class="ContainerRow">
            </Panel>
        </Panel>
            <Panel hittest="false" class="Hidden" id="HideButtonContainer">
        </Panel>
    </Panel>

</root>
